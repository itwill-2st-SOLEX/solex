<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.itwillbs.solex.login.LoginMapper">
	<select id="loginEmp" resultType="map">
		select *
		  from employee
		 where emp_num = #{emp_num}
		   and emp_pw = #{emp_pw}
	</select>

</mapper>